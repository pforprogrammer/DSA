#include <iostream>
using namespace std;
void reverse(int arr[],int size){
    int start=0;
    int end=size-1;
    while(start<=end){
        swap(arr[start],arr[end]);
        start++;
        end--;
    }
    for(int i=0; i<size; i++){
        cout<<arr[i]<<" ";
    }
}
int main() {
   int arr[5]={54,4,0,2,-2347};
   reverse(arr,5);
   
    return 0;
}
